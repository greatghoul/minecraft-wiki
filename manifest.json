 {
  "name": "Minecraft Wiki",
  "description": "Visit minecraft wiki pages from omnibox prefix: 'mc'",
  "manifest_version": 2,
  "version": "1.0",
  "omnibox": {
    "keyword" : "mc"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "*://minecraft-zh.gamepedia.com/*"
  ]
}